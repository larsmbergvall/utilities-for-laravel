{
  "name": "larsmbergvall/utilities-for-laravel",
  "description": "Some utilities and helpers for use in Laravel projects",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Larsmbergvall\\UtilitiesForLaravel\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Lars Bergvall",
      "email": "lars.m.bergvall@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larsmbergvall\\UtilitiesForLaravel\\UtilitiesForLaravelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
